{
  "Name": "Basic Deployment",
  "Settings":
  {
    "Heartbeat":
    {
      "Enabled": true,
      "Interval": 500,
      "Tolerance": 1000
    },
    "Control Buffer":
    {
      "Capacity": 512,
      "Size": 65536
    }
  },
  "Partitions": [
    {
      "Name": "Entry Partition",
      "Tasks": [
        {
          "Function Name": "Listen Request",
          "Inputs": [],
          "Outputs": [
            "Request"
          ]
        }
      ],
      "Replicas":
      [
        { "Name": "Replica 0/0" }
      ]
    },
    {
      "Name": "Processor Partition",
      "Tasks": [
        {
          "Function Name": "Process Request",
          "Inputs": [
            "Request"
          ],
          "Outputs": [
            "Processed Request"
          ]
        }
      ],
      "Replicas":
      [
        { "Name": "Replica 1/0" }
      ]
    },
    {
      "Name": "Result Partition",
      "Tasks": [
        {
          "Function Name": "Return Result",
          "Inputs": [
            "Processed Request"
          ],
          "Outputs": []
        }
      ],
      "Replicas":
      [
        { "Name": "Replica 2/0" }
      ]
    }
  ],
  "Edges": [
    {
      "Name": "Request",
      "Communication Method": "Host Network",
      "Buffer Capacity": 2,
      "Buffer Size": 4096,
      "Mode": "Copy"
    },
    {
      "Name": "Processed Request",
      "Communication Method": "Ascend NPU Network",
      "Buffer Capacity": 2,
      "Buffer Size": 16777216,
      "Mode": "Copy"
    }
  ]
}
